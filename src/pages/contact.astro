---
import Layout from '@/layouts/Layout.astro';
import Header from '@/components/Header.astro';
import ContactForm from '@/components/ContactForm.astro';
import ContactInfo from '@/components/ContactInfo.astro';
import CTAResume from '@/components/CTAResume.astro';
import {
    secretKey,
} from '@/hooks/env.js';
import Breadcrumb from '@/components/Breadcrumb.astro';
---

<Layout 
	title="Contacto"
	canonical="https://solucionesagrovialeshm.com.ar/contact"
	metaTitle="Soluciones Agroviales H.M. - Contacto"
	metaDescription="Contáctenos ahora para obtener una cotización. Obtenga cotización sin cargo,Repuestos, servicio y reparación de equipos de maquinarias viales"
	metaImg="https://solucionesagrovialeshm.com.ar/images/logo-meta.webp"
>
    <Header />
    <Breadcrumb 
		page="Contacto"
		title="Contáctenos"
		description="Obtenga cotización sin cargo"
	/>
    <ContactInfo />
    <ContactForm />
    <CTAResume />
</Layout>
<!-- recaptcha v3 -->
<script async src={'https://www.google.com/recaptcha/api.js?render=' + secretKey}></script>