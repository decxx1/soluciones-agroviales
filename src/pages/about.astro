import AboutPresentation from '@/components/AboutPresentation.astro';
import AboutLogo from '@/components/AboutLogo.astro';
import CTAResume from '@/components/CTAResume.astro';
---

<Layout 
	title="Sobre Nosotros"
	canonical="https://solucionesagrovialeshm.com.ar/about"
	metaTitle="Soluciones Agroviales H.M. - Sobre Nosotros"
	metaDescription="Nuestro equipo está compuesto por técnicos mecánicos, electricistas y electrónicos. Con más de 30 años ofrecemos Soluciones completas para equipos viales"
	metaImg="https://solucionesagrovialeshm.com.ar/images/about.webp"
	preLoad="images/about.webp"
>
    <Header />
    <AboutPresentation />
    <AboutLogo />
    <CTAResume />
