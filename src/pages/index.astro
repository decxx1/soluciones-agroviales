---
import Layout from '@/layouts/Layout.astro';
import Hero from '@/components/Hero.astro';
import Header from '@/components/Header.astro';
import Feature from '@/components/Feature.astro';
import FeatureDiagnosis from '@/components/FeatureDiagnosis.astro';
import FeatureParts from '@/components/FeatureParts.astro';
import CTABrands from '@/components/CTABrands.astro';
import CTAServices from '@/components/CTAServices.astro';
import FieldServices from '@/components/FieldServices.astro';
import Brands from '@/components/Brands.astro';
---

<Layout 
	title="Soluciones Agroviales H.M."
	canonical="https://solucionesagrovialeshm.com.ar"
	metaTitle="Soluciones Agroviales H.M."
	metaDescription="Servicio y reparación de equipos de maquinarias viales y venta de repuestos. Servicios especializados en ,hidráulica, mecánica, eléctrica y electrónica"
	metaImg="https://solucionesagrovialeshm.com.ar/images/excavadora.webp"
	preLoad="images/excavadora.webp"
>
	<Hero />
	<Header />
	<Feature />
	<CTABrands />
	<FeatureDiagnosis />
	<hr class="max-w-screen-sm mx-auto border-gray-500 dark:border-gray-600 border-1" />
	<FeatureParts />
	<CTAServices />
	<FieldServices />
	<Brands />
</Layout>
